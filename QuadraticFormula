def quad_eq(a, b, c):
    sqrt1 = (-b-math.sqrt((b**2) - (4*a*c)))/(2*a)
    sqrt2 = (-b+math.sqrt((b**2) - (4*a*c)))/(2*a)

    return 'The solutions are {0} and {1}'.format(sqrt1,sqrt2)

print(quad_eq(2, 11, 5))
     
